{
  "name": "citr",
  "version": "1.0.0",
  "description": "Static version of long standing music blog",
  "author": "fightwithcrayon <contact@andrewthomashill.co.uk>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "git checkout gh-pages; push-dir --dir=dist --branch=gh-pages --cleanup; git checkout 2018"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.1.1",
    "date-fns": "^1.29.0",
    "intersection-observer": "^0.5.0",
    "nuxt": "^1.0.0",
    "nuxt-generate-cluster": "^1.3.0",
    "vue-intersect": "^1.1.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.7.2",
    "push-dir": "^0.4.1",
    "sass-loader": "^6.0.7"
  }
}
